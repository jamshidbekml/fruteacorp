model Address {
    id             String  @id @default(uuid()) @db.Uuid
    userId         String  @db.Uuid
    streetName     String
    houseEntryCode String?
    houseLine      String?
    houseNumber    String?
    houseStage     String?

    User Users @relation(fields: [userId], references: [id], onDelete: Cascade)
    orders Orders[]

    @@map("address")
}
